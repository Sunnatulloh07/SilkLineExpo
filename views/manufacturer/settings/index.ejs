<%- include('../partials/header', { title: t('manufacturer.settings.pageTitle'), lng: lng, user: user }) %>
<%- include('../partials/sidebar', { user: user, currentPage: 'settings' }) %>

<!-- Enhanced CSS Imports - Professional Settings Style -->
<link rel="stylesheet" href="/css/products-management-redesign.css">
<link rel="stylesheet" href="/css/products-table-view.css">
<link rel="stylesheet" href="/css/orders-management.css">
<link rel="stylesheet" href="/css/manufacturer-cards.css">
<link rel="stylesheet" href="/css/manufacturer-theme.css">
<link rel="stylesheet" href="/css/toast-notifications.css">
<link rel="stylesheet" href="/css/modal-system.css">
<link rel="stylesheet" href="/css/manufacturer/settings.css">

<!-- Security: CSRF Token -->
<meta name="csrf-token" content="<%= typeof csrfToken !== 'undefined' ? csrfToken : '' %>">

<!-- Main Content -->
<main class="admin-main">
  <div class="admin-content">
    
    <!-- Professional Settings Header -->
    <div class="orders-header animate-fade-in">
      <div class="orders-header-content">
        <div class="orders-title-section">
          <h1 class="orders-main-title">
            <i class="fas fa-cog"></i>
            <%= t('manufacturer.settings.pageTitle') %>
          </h1>
          <p class="orders-subtitle"><%= t('manufacturer.settings.pageSubtitle') %></p>
        </div>
        <div class="orders-header-actions">
          <button class="orders-btn orders-btn-primary" id="saveAllSettingsBtn">
            <i class="fas fa-save"></i>
            <%= t('manufacturer.settings.actions.saveAll') %>
          </button>
          <button class="orders-btn orders-btn-outline" id="resetSettingsBtn">
            <i class="fas fa-undo"></i>
            <%= t('manufacturer.settings.actions.reset') %>
          </button>
          <button class="orders-btn orders-btn-secondary" id="exportSettingsBtn">
            <i class="fas fa-download"></i>
            <%= t('manufacturer.settings.actions.export') %>
          </button>
        </div>
      </div>
    </div>

    <!-- Settings Navigation Tabs -->
    <div class="settings-nav-tabs animate-fade-in">
      <div class="settings-tabs-container">
        <button class="settings-tab active" data-tab="company">
          <i class="fas fa-building"></i>
          <span><%= t('manufacturer.settings.tabs.company') %></span>
        </button>
        <button class="settings-tab" data-tab="contact">
          <i class="fas fa-address-book"></i>
          <span><%= t('manufacturer.settings.tabs.contact') %></span>
        </button>
        <button class="settings-tab" data-tab="business">
          <i class="fas fa-briefcase"></i>
          <span><%= t('manufacturer.settings.tabs.business') %></span>
        </button>
        <button class="settings-tab" data-tab="security">
          <i class="fas fa-shield-alt"></i>
          <span><%= t('manufacturer.settings.tabs.security') %></span>
        </button>
        <button class="settings-tab" data-tab="preferences">
          <i class="fas fa-sliders-h"></i>
          <span><%= t('manufacturer.settings.tabs.preferences') %></span>
        </button>
        <button class="settings-tab" data-tab="integrations">
          <i class="fas fa-plug"></i>
          <span><%= t('manufacturer.settings.tabs.integrations') %></span>
        </button>
      </div>
    </div>

    <!-- Settings Content Container -->
    <div class="settings-content-container animate-fade-in">
      
      <!-- Company Information Tab -->
      <div class="settings-tab-content active" id="company-tab">
        <form id="companyInfoForm" class="settings-form" novalidate>
          <div class="settings-section">
            <div class="settings-section-header">
              <h3 class="settings-section-title">
                <i class="fas fa-building"></i>
                <%= t('manufacturer.settings.company.title') %>
              </h3>
              <p class="settings-section-subtitle"><%= t('manufacturer.settings.company.subtitle') %></p>
            </div>
            
            <!-- Form Messages Container -->
            <div class="form-messages" id="companyFormMessages" style="display: none;">
              <div class="message-content"></div>
            </div>
            
            <div class="settings-form-grid">
            <!-- Company Logo -->
            <div class="settings-form-group full-width">
              <label class="settings-label"><%= t('manufacturer.settings.company.logo.title') %></label>
              <div class="logo-upload-container">
                <div class="current-logo">
                  <img src="<%= (user.companyLogo && user.companyLogo.url) || '/assets/images/avatars/default-company.png' %>" 
                       alt="<%= user.companyName || t('manufacturer.settings.company.logo.title') %>" 
                       id="currentLogoPreview"
                       onerror="this.onerror=null; this.src='/assets/images/avatars/default.png'">
                </div>
                <div class="logo-upload-actions">
                  <input type="file" id="logoUpload" accept="image/*" class="hidden">
                  <button type="button" class="orders-btn orders-btn-primary" id="logoUploadBtn">
                    <i class="fas fa-upload"></i>
                    <%= t('manufacturer.settings.company.logo.update') %>
                  </button>
                  <small class="logo-upload-help"><%= t('manufacturer.settings.company.logo.help') %></small>
                </div>
              </div>
            </div>

            <!-- Company Name -->
            <div class="settings-form-group">
              <label class="settings-label" for="companyName"><%= t('manufacturer.settings.company.name.label') %></label>
              <input type="text" id="companyName" name="companyName" class="settings-input" 
                     value="<%= user.companyName || '' %>" 
                     required 
                     minlength="2" 
                     maxlength="200"
                     pattern="[a-zA-Z0-9\s\-&.,']+"
                     autocomplete="organization">
              <small class="settings-help"><%= t('manufacturer.settings.company.name.help') %></small>
              <div class="field-error" style="display: none;"></div>
            </div>

            <!-- Activity Type -->
            <div class="settings-form-group">
              <label class="settings-label" for="activityType"><%= t('manufacturer.settings.company.activityType.label') %></label>
              <select id="activityType" name="activityType" class="settings-select" required>
                <option value=""><%= t('manufacturer.settings.company.activityType.select') %></option>
                <option value="textiles_clothing" <%= user.activityType === 'textiles_clothing' ? 'selected' : '' %>><%= t('manufacturer.settings.company.activityType.options.textiles_clothing') %></option>
                <option value="food_beverages" <%= user.activityType === 'food_beverages' ? 'selected' : '' %>><%= t('manufacturer.settings.company.activityType.options.food_beverages') %></option>
                <option value="electronics" <%= user.activityType === 'electronics' ? 'selected' : '' %>><%= t('manufacturer.settings.company.activityType.options.electronics') %></option>
                <option value="machinery_equipment" <%= user.activityType === 'machinery_equipment' ? 'selected' : '' %>><%= t('manufacturer.settings.company.activityType.options.machinery_equipment') %></option>
                <option value="chemicals" <%= user.activityType === 'chemicals' ? 'selected' : '' %>><%= t('manufacturer.settings.company.activityType.options.chemicals') %></option>
                <option value="agriculture" <%= user.activityType === 'agriculture' ? 'selected' : '' %>><%= t('manufacturer.settings.company.activityType.options.agriculture') %></option>
                <option value="construction_materials" <%= user.activityType === 'construction_materials' ? 'selected' : '' %>><%= t('manufacturer.settings.company.activityType.options.construction_materials') %></option>
                <option value="automotive" <%= user.activityType === 'automotive' ? 'selected' : '' %>><%= t('manufacturer.settings.company.activityType.options.automotive') %></option>
                <option value="other" <%= user.activityType === 'other' ? 'selected' : '' %>><%= t('manufacturer.settings.company.activityType.options.other') %></option>
              </select>
            </div>

            <!-- Tax Number -->
            <div class="settings-form-group">
              <label class="settings-label" for="taxNumber"><%= t('manufacturer.settings.company.taxNumber.label') %></label>
              <input type="text" id="taxNumber" name="taxNumber" class="settings-input" 
                     value="<%= user.taxNumber || '' %>" 
                     required 
                     minlength="6" 
                     maxlength="20"
                     pattern="[0-9]+"
                     autocomplete="tax-id">
              <small class="settings-help"><%= t('manufacturer.settings.company.taxNumber.help') %></small>
              <div class="field-error" style="display: none;"></div>
            </div>

            <!-- Established Year -->
            <div class="settings-form-group">
              <label class="settings-label" for="establishedYear"><%= t('manufacturer.settings.company.establishedYear.label') %></label>
              <input type="number" id="establishedYear" name="establishedYear" class="settings-input" 
                     value="<%= user.establishedYear || '' %>" 
                     min="1900" 
                     max="<%= new Date().getFullYear() %>"
                     step="1"
                     autocomplete="bday-year">
              <small class="settings-help"><%= t('manufacturer.settings.company.establishedYear.help') %></small>
              <div class="field-error" style="display: none;"></div>
            </div>

            <!-- Employee Count -->
            <div class="settings-form-group">
              <label class="settings-label" for="employeeCount"><%= t('manufacturer.settings.company.employeeCount.label') %></label>
              <select id="employeeCount" name="employeeCount" class="settings-select">
                <option value=""><%= t('manufacturer.settings.company.employeeCount.select') %></option>
                <option value="1-10" <%= user.employeeCount === '1-10' ? 'selected' : '' %>><%= t('manufacturer.settings.company.employeeCount.options.1-10') %></option>
                <option value="11-50" <%= user.employeeCount === '11-50' ? 'selected' : '' %>><%= t('manufacturer.settings.company.employeeCount.options.11-50') %></option>
                <option value="51-200" <%= user.employeeCount === '51-200' ? 'selected' : '' %>><%= t('manufacturer.settings.company.employeeCount.options.51-200') %></option>
                <option value="201-500" <%= user.employeeCount === '201-500' ? 'selected' : '' %>><%= t('manufacturer.settings.company.employeeCount.options.201-500') %></option>
                <option value="500+" <%= user.employeeCount === '500+' ? 'selected' : '' %>><%= t('manufacturer.settings.company.employeeCount.options.500+') %></option>
              </select>
            </div>

            <!-- Annual Revenue -->
            <div class="settings-form-group">
              <label class="settings-label" for="annualRevenue"><%= t('manufacturer.settings.company.annualRevenue.label') %></label>
              <select id="annualRevenue" name="annualRevenue" class="settings-select">
                <option value=""><%= t('manufacturer.settings.company.annualRevenue.select') %></option>
                <option value="under-100k" <%= user.annualRevenue === 'under-100k' ? 'selected' : '' %>><%= t('manufacturer.settings.company.annualRevenue.options.under-100k') %></option>
                <option value="100k-500k" <%= user.annualRevenue === '100k-500k' ? 'selected' : '' %>><%= t('manufacturer.settings.company.annualRevenue.options.100k-500k') %></option>
                <option value="500k-1m" <%= user.annualRevenue === '500k-1m' ? 'selected' : '' %>><%= t('manufacturer.settings.company.annualRevenue.options.500k-1m') %></option>
                <option value="1m-5m" <%= user.annualRevenue === '1m-5m' ? 'selected' : '' %>><%= t('manufacturer.settings.company.annualRevenue.options.1m-5m') %></option>
                <option value="5m+" <%= user.annualRevenue === '5m+' ? 'selected' : '' %>><%= t('manufacturer.settings.company.annualRevenue.options.5m+') %></option>
              </select>
            </div>

            <!-- Company Description -->
            <div class="settings-form-group full-width">
              <label class="settings-label" for="companyDescription"><%= t('manufacturer.settings.company.description.label') %></label>
              <textarea id="companyDescription" name="companyDescription" class="settings-textarea" rows="4"
                        placeholder="<%= t('manufacturer.settings.company.description.placeholder') %>"><%= user.description || '' %></textarea>
              <small class="settings-help"><%= t('manufacturer.settings.company.description.help') %></small>
            </div>
          </div>

            <div class="settings-section-actions">
              <button type="submit" class="orders-btn orders-btn-primary" id="saveCompanyInfo">
                <i class="fas fa-save"></i>
                <%= t('manufacturer.settings.actions.save') %>
              </button>
              <button type="reset" class="orders-btn orders-btn-secondary">
                <i class="fas fa-undo"></i>
                <%= t('manufacturer.settings.actions.reset') %>
              </button>
            </div>
          </div>
        </form>
      </div>

      <!-- Contact Information Tab -->
      <div class="settings-tab-content" id="contact-tab">
        <form id="contactInfoForm" class="settings-form" novalidate>
          <div class="settings-section">
            <div class="settings-section-header">
              <h3 class="settings-section-title">
                <i class="fas fa-address-book"></i>
                <%= t('manufacturer.settings.contact.title') %>
              </h3>
              <p class="settings-section-subtitle"><%= t('manufacturer.settings.contact.subtitle') %></p>
            </div>
            
            <!-- Form Messages Container -->
            <div class="form-messages" id="contactFormMessages" style="display: none;">
              <div class="message-content"></div>
            </div>
            
            <div class="settings-form-grid">
              <!-- Email -->
              <div class="settings-form-group">
                <label class="settings-label" for="email"><%= t('manufacturer.settings.contact.email.label') %></label>
                <input type="email" id="email" name="email" class="settings-input" 
                       value="<%= user.email || '' %>" 
                       required 
                       pattern="[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,}$"
                       autocomplete="email">
                <small class="settings-help"><%= t('manufacturer.settings.contact.email.help') %></small>
                <div class="field-error" style="display: none;"></div>
              </div>

              <!-- Phone -->
              <div class="settings-form-group">
                <label class="settings-label" for="phone"><%= t('manufacturer.settings.contact.phone.label') %></label>
                <input type="tel" id="phone" name="phone" class="settings-input" 
                       value="<%= user.phone || '' %>" 
                       required 
                       pattern="^\+\d{1,4}\d{6,14}$"
                       autocomplete="tel"
                       placeholder="<%= t('manufacturer.settings.contact.phone.placeholder') %>">
                <small class="settings-help"><%= t('manufacturer.settings.contact.phone.help') %></small>
                <div class="field-error" style="display: none;"></div>
              </div>

              <!-- Website -->
              <div class="settings-form-group">
                <label class="settings-label" for="website"><%= t('manufacturer.settings.contact.website.label') %></label>
                <input type="url" id="website" name="website" class="settings-input" 
                       value="<%= user.website || '' %>" 
                       placeholder="<%= t('manufacturer.settings.contact.website.placeholder') %>"
                       pattern="https?://.*"
                       autocomplete="url">
                <small class="settings-help"><%= t('manufacturer.settings.contact.website.help') %></small>
                <div class="field-error" style="display: none;"></div>
              </div>

              <!-- Address -->
              <div class="settings-form-group full-width">
                <label class="settings-label" for="address"><%= t('manufacturer.settings.contact.address.label') %></label>
                <textarea id="address" name="address" class="settings-textarea" rows="3" 
                          required 
                          minlength="10" 
                          maxlength="500"
                          autocomplete="street-address"><%= user.address || '' %></textarea>
                <small class="settings-help"><%= t('manufacturer.settings.contact.address.help') %></small>
                <div class="field-error" style="display: none;"></div>
              </div>

            <!-- Country -->
            <div class="settings-form-group">
              <label class="settings-label" for="country"><%= t('manufacturer.settings.contact.country.label') %></label>
              <select id="country" name="country" class="settings-select" required>
                <option value=""><%= t('manufacturer.settings.contact.country.select') %></option>
                <option value="Uzbekistan" <%= user.country === 'Uzbekistan' ? 'selected' : '' %>><%= t('manufacturer.settings.contact.country.options.Uzbekistan') %></option>
                <option value="Kazakhstan" <%= user.country === 'Kazakhstan' ? 'selected' : '' %>><%= t('manufacturer.settings.contact.country.options.Kazakhstan') %></option>
                <option value="Kyrgyzstan" <%= user.country === 'Kyrgyzstan' ? 'selected' : '' %>><%= t('manufacturer.settings.contact.country.options.Kyrgyzstan') %></option>
                <option value="Tajikistan" <%= user.country === 'Tajikistan' ? 'selected' : '' %>><%= t('manufacturer.settings.contact.country.options.Tajikistan') %></option>
                <option value="Turkmenistan" <%= user.country === 'Turkmenistan' ? 'selected' : '' %>><%= t('manufacturer.settings.contact.country.options.Turkmenistan') %></option>
              </select>
            </div>

            <!-- City -->
            <div class="settings-form-group">
              <label class="settings-label" for="city"><%= t('manufacturer.settings.contact.city.label') %></label>
              <input type="text" id="city" name="city" class="settings-input" 
                     value="<%= user.city || '' %>" required>
            </div>
          </div>

          <!-- Social Media Section -->
          <div class="settings-subsection">
            <h4 class="settings-subsection-title">
              <i class="fas fa-share-alt"></i>
              <%= t('manufacturer.settings.contact.socialMedia.title') %>
            </h4>
            
            <div class="settings-form-grid">
              <!-- LinkedIn -->
              <div class="settings-form-group">
                <label class="settings-label" for="linkedin"><%= t('manufacturer.settings.contact.socialMedia.linkedin') %></label>
                <div class="input-with-icon">
                  <i class="fab fa-linkedin"></i>
                  <input type="url" id="linkedin" name="socialMedia.linkedin" class="settings-input" 
                         value="<%= user.socialMedia?.linkedin || '' %>" placeholder="https://linkedin.com/company/...">
                </div>
              </div>

              <!-- Facebook -->
              <div class="settings-form-group">
                <label class="settings-label" for="facebook"><%= t('manufacturer.settings.contact.socialMedia.facebook') %></label>
                <div class="input-with-icon">
                  <i class="fab fa-facebook"></i>
                  <input type="url" id="facebook" name="socialMedia.facebook" class="settings-input" 
                         value="<%= user.socialMedia?.facebook || '' %>" placeholder="https://facebook.com/...">
                </div>
              </div>

              <!-- Instagram -->
              <div class="settings-form-group">
                <label class="settings-label" for="instagram"><%= t('manufacturer.settings.contact.socialMedia.instagram') %></label>
                <div class="input-with-icon">
                  <i class="fab fa-instagram"></i>
                  <input type="url" id="instagram" name="socialMedia.instagram" class="settings-input" 
                         value="<%= user.socialMedia?.instagram || '' %>" placeholder="https://instagram.com/...">
                </div>
              </div>

              <!-- Telegram -->
              <div class="settings-form-group">
                <label class="settings-label" for="telegram"><%= t('manufacturer.settings.contact.socialMedia.telegram') %></label>
                <div class="input-with-icon">
                  <i class="fab fa-telegram"></i>
                  <input type="text" id="telegram" name="socialMedia.telegram" class="settings-input" 
                         value="<%= user.socialMedia?.telegram || '' %>" placeholder="@username">
                </div>
              </div>
            </div>
          </div>

            <div class="settings-section-actions">
              <button type="submit" class="orders-btn orders-btn-primary" id="saveContactInfo">
                <i class="fas fa-save"></i>
                <%= t('manufacturer.settings.actions.save') %>
              </button>
              <button type="reset" class="orders-btn orders-btn-secondary">
                <i class="fas fa-undo"></i>
                <%= t('manufacturer.settings.actions.reset') %>
              </button>
            </div>
          </div>
        </form>
      </div>

      <!-- Business Information Tab -->
      <div class="settings-tab-content" id="business-tab">
        <div class="settings-section">
          <div class="settings-section-header">
            <h3 class="settings-section-title">
              <i class="fas fa-briefcase"></i>
              <%= t('manufacturer.settings.business.title') %>
            </h3>
            <p class="settings-section-subtitle"><%= t('manufacturer.settings.business.subtitle') %></p>
          </div>
          
          <div class="settings-form-grid">
            <!-- Business License -->
            <div class="settings-form-group">
              <label class="settings-label" for="businessLicense"><%= t('manufacturer.settings.business.license.label') %></label>
              <input type="text" id="businessLicense" name="businessLicense" class="settings-input" 
                     value="<%= user.businessLicense || '' %>">
              <small class="settings-help"><%= t('manufacturer.settings.business.license.help') %></small>
            </div>

            <!-- Company Type -->
            <div class="settings-form-group">
              <label class="settings-label" for="companyType"><%= t('manufacturer.settings.business.type.label') %></label>
              <select id="companyType" name="companyType" class="settings-select" disabled>
                <option value="manufacturer" <%= user.companyType === 'manufacturer' ? 'selected' : '' %>><%= t('manufacturer.settings.business.type.options.manufacturer') %></option>
                <option value="distributor" <%= user.companyType === 'distributor' ? 'selected' : '' %>><%= t('manufacturer.settings.business.type.options.distributor') %></option>
              </select>
              <small class="settings-help"><%= t('manufacturer.settings.business.type.help') %></small>
            </div>

            <!-- Status -->
            <div class="settings-form-group">
              <label class="settings-label" for="status"><%= t('manufacturer.settings.business.status.label') %></label>
              <div class="status-display">
                <span class="status-badge status-<%= user.status %>">
                  <% if (user.status === 'active') { %>
                    <i class="fas fa-check-circle"></i> <%= t('manufacturer.settings.business.status.options.active') %>
                  <% } else if (user.status === 'pending') { %>
                    <i class="fas fa-clock"></i> <%= t('manufacturer.settings.business.status.options.pending') %>
                  <% } else if (user.status === 'blocked') { %>
                    <i class="fas fa-ban"></i> <%= t('manufacturer.settings.business.status.options.blocked') %>
                  <% } else if (user.status === 'suspended') { %>
                    <i class="fas fa-pause"></i> <%= t('manufacturer.settings.business.status.options.suspended') %>
                  <% } %>
                </span>
              </div>
              <small class="settings-help"><%= t('manufacturer.settings.business.status.help') %></small>
            </div>
          </div>

          <!-- Manufacturing Capabilities Section -->
          <div class="settings-subsection">
            <h4 class="settings-subsection-title">
              <i class="fas fa-industry"></i>
              <%= t('manufacturer.settings.business.capabilities.title') %>
            </h4>
            
            <div class="capabilities-grid">
              <div class="capability-item">
                <div class="capability-header">
                  <i class="fas fa-tshirt"></i>
                  <h5><%= t('manufacturer.settings.business.capabilities.textile.title') %></h5>
                </div>
                <div class="capability-details">
                  <label class="checkbox-label">
                    <input type="checkbox" name="capabilities" value="textile_manufacturing">
                    <span class="checkmark"></span>
                    <%= t('manufacturer.settings.business.capabilities.textile.description') %>
                  </label>
                </div>
              </div>

              <div class="capability-item">
                <div class="capability-header">
                  <i class="fas fa-cut"></i>
                  <h5><%= t('manufacturer.settings.business.capabilities.garment.title') %></h5>
                </div>
                <div class="capability-details">
                  <label class="checkbox-label">
                    <input type="checkbox" name="capabilities" value="garment_manufacturing">
                    <span class="checkmark"></span>
                    <%= t('manufacturer.settings.business.capabilities.garment.description') %>
                  </label>
                </div>
              </div>

              <div class="capability-item">
                <div class="capability-header">
                  <i class="fas fa-palette"></i>
                  <h5><%= t('manufacturer.settings.business.capabilities.dyeing.title') %></h5>
                </div>
                <div class="capability-details">
                  <label class="checkbox-label">
                    <input type="checkbox" name="capabilities" value="dyeing_printing">
                    <span class="checkmark"></span>
                    <%= t('manufacturer.settings.business.capabilities.dyeing.description') %>
                  </label>
                </div>
              </div>

              <div class="capability-item">
                <div class="capability-header">
                  <i class="fas fa-medal"></i>
                  <h5><%= t('manufacturer.settings.business.capabilities.quality.title') %></h5>
                </div>
                <div class="capability-details">
                  <label class="checkbox-label">
                    <input type="checkbox" name="capabilities" value="quality_control">
                    <span class="checkmark"></span>
                    <%= t('manufacturer.settings.business.capabilities.quality.description') %>
                  </label>
                </div>
              </div>

              <div class="capability-item">
                <div class="capability-header">
                  <i class="fas fa-shipping-fast"></i>
                  <h5><%= t('manufacturer.settings.business.capabilities.delivery.title') %></h5>
                </div>
                <div class="capability-details">
                  <label class="checkbox-label">
                    <input type="checkbox" name="capabilities" value="fast_delivery">
                    <span class="checkmark"></span>
                    <%= t('manufacturer.settings.business.capabilities.delivery.description') %>
                  </label>
                </div>
              </div>

              <div class="capability-item">
                <div class="capability-header">
                  <i class="fas fa-tools"></i>
                  <h5><%= t('manufacturer.settings.business.capabilities.custom.title') %></h5>
                </div>
