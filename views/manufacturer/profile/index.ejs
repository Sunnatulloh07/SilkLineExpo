    upload: t('manufacturer.profile.certifications.upload'),
    add: t('manufacturer.profile.certifications.add'),
    status: {
      active: t('manufacturer.profile.certifications.status.active')
    },
    examples: {
      iso9001: {
        name: t('manufacturer.profile.certifications.examples.iso9001.name'),
