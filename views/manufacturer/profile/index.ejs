<%- include('../partials/header', { title: t('manufacturer.profile.pageTitle'), lng: lng, user: user }) %>
<%- include('../partials/sidebar', { user: user, currentPage: 'profile' }) %>

<!-- Enhanced CSS Imports - Professional Profile Style -->
<link rel="stylesheet" href="/css/products-management-redesign.css">
<link rel="stylesheet" href="/css/products-table-view.css">
<link rel="stylesheet" href="/css/orders-management.css">
<link rel="stylesheet" href="/css/manufacturer-cards.css">
<link rel="stylesheet" href="/css/manufacturer-theme.css">
<link rel="stylesheet" href="/css/toast-notifications.css">
<link rel="stylesheet" href="/css/modal-system.css">
<link rel="stylesheet" href="/css/manufacturer/profile.css">

<!-- Security: CSRF Token -->
<meta name="csrf-token" content="<%= typeof csrfToken !== 'undefined' ? csrfToken : '' %>">

<!-- External Scripts -->
<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
<script src="https://cdn.jsdelivr.net/npm/qrcode@1.5.3/build/qrcode.min.js"></script>

<!-- Main Content -->
<main class="admin-main">
  <div class="admin-content profile-page">
    
    <!-- Single Page Loading State -->
    <div id="pageLoadingOverlay" class="page-loading-overlay">
      <div class="page-loading-content">
        <div class="page-loading-spinner">
          <i class="fas fa-spinner fa-spin"></i>
        </div>
        <h3><%= t('manufacturer.profile.messages.loading', 'Ma\'lumot yuklanmoqda...') %></h3>
        <p><%= t('manufacturer.profile.messages.loadingSubtext', 'Profil ma\'lumotlari yuklanmoqda') %></p>
      </div>
    </div>
    
    <!-- Professional Profile Header -->
    <div class="orders-header animate-fade-in">
      <div class="orders-header-content">
        <div class="orders-title-section">
          <h1 class="orders-main-title">
            <i class="fas fa-user-circle"></i>
            <%= t('manufacturer.profile.pageTitle') %>
          </h1>
          <p class="orders-subtitle"><%= t('manufacturer.profile.pageSubtitle') %></p>
        </div>
      </div>
    </div>

    <!-- Company Overview Section -->
    <div class="profile-overview-section animate-fade-in">
      <div class="profile-hero-card">
        <div class="profile-hero-content">
          <div class="company-logo-section">
            <div class="company-logo-container">
              <img src="<%= (user.companyLogo && user.companyLogo.url) || '/assets/images/avatars/default-company.png' %>" 
                   alt="<%= user.companyName || t('manufacturer.profile.company.logo') %>" 
                   class="company-logo"
                   id="companyLogo"
                   onerror="this.onerror=null; this.src='/assets/images/avatars/default.png'">
            </div>
            <div class="company-verification">
              <% if (user.status === 'active') { %>
                <span class="verification-badge verified">
                  <i class="fas fa-check-circle"></i>
                  <%= t('manufacturer.profile.verification.verified') %>
                </span>
              <% } else { %>
                <span class="verification-badge pending">
                  <i class="fas fa-clock"></i>
                  <%= t('manufacturer.profile.verification.pending') %>
                </span>
              <% } %>
            </div>
          </div>

          <div class="company-basic-info">
            <h2 class="company-name" id="companyName">-</h2>
            <div class="company-meta">
              <span class="company-type">
                <i class="fas fa-industry"></i>
                <%= t('manufacturer.profile.company.type') %>
              </span>
              <span class="company-location">
                <i class="fas fa-map-marker-alt"></i>
                <span id="companyLocation">-</span>
              </span>
              <span class="company-established" id="companyEstablished" style="display: none;">
                <i class="fas fa-calendar"></i>
                <span id="establishedYear">-</span> <%= t('manufacturer.profile.company.established') %>
              </span>
            </div>
            <div class="company-contact-quick">
              <a href="#" class="contact-btn email" id="heroEmail">
                <i class="fas fa-envelope"></i>
                -
              </a>
              <a href="#" class="contact-btn phone" id="heroPhone">
                <i class="fas fa-phone"></i>
                -
              </a>
              <a href="#" target="_blank" class="contact-btn website" id="heroWebsite" style="display: none;">
                <i class="fas fa-globe"></i>
                <%= t('manufacturer.profile.contact.website') %>
              </a>
            </div>
          </div>

            <div class="company-stats-quick">
            <div class="stat-item">
              <div class="stat-value" id="totalProducts">0</div>
              <div class="stat-label"><%= t('manufacturer.profile.company.stats.products') %></div>
            </div>
            <div class="stat-item">
              <div class="stat-value" id="totalOrders">0</div>
              <div class="stat-label"><%= t('manufacturer.profile.company.stats.orders') %></div>
            </div>
            <div class="stat-item">
              <div class="stat-value" id="totalRevenue">$0</div>
              <div class="stat-label"><%= t('manufacturer.profile.company.stats.revenue') %></div>
            </div>
            <div class="stat-item">
              <div class="stat-value" id="customerRating">0.0</div>
              <div class="stat-label"><%= t('manufacturer.profile.company.stats.rating') %></div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Profile Content Grid -->
    <div class="profile-content-grid animate-fade-in">
      
      <!-- Left Column -->
      <div class="profile-left-column">
        
        <!-- Business Information Card -->
        <div class="profile-card">
          <div class="profile-card-header">
            <h3 class="profile-card-title">
              <i class="fas fa-briefcase"></i>
              <%= t('manufacturer.profile.business.title') %>
            </h3>
          </div>
          <div class="profile-card-content">
            <div class="info-grid">
              <div class="info-item">
                <label><%= t('manufacturer.profile.business.activityType') %>:</label>
                <span class="activity-type" id="activityType">
                  <i class="fas fa-industry"></i> -
                </span>
              </div>

              <div class="info-item">
                <label><%= t('manufacturer.profile.business.license') %>:</label>
                <span id="businessLicense">-</span>
              </div>

              <div class="info-item">
                <label><%= t('manufacturer.profile.business.taxNumber') %>:</label>
                <span id="taxNumber">-</span>
              </div>

              <div class="info-item">
                <label><%= t('manufacturer.profile.business.employeeCount') %>:</label>
                <span class="employee-count" id="employeeCount">
                  <i class="fas fa-users"></i>
                  -
                </span>
              </div>

              <div class="info-item">
                <label><%= t('manufacturer.profile.business.annualRevenue') %>:</label>
                <span class="annual-revenue" id="annualRevenue">
                  <i class="fas fa-dollar-sign"></i>
                  -
                </span>
              </div>
            </div>
          </div>
        </div>

        <!-- Manufacturing Capabilities Card -->
        <div class="profile-card">
          <div class="profile-card-header">
            <h3 class="profile-card-title">
              <i class="fas fa-industry"></i>
              <%= t('manufacturer.profile.capabilities.title') %>
            </h3>
          </div>
          <div class="profile-card-content">
            <div class="capabilities-grid">
              <div class="capability-item">
                <div class="capability-icon">
                  <i class="fas fa-tshirt"></i>
                </div>
                <div class="capability-info">
                  <h5><%= t('manufacturer.profile.capabilities.textile.title') %></h5>
                  <p><%= t('manufacturer.profile.capabilities.textile.description') %></p>
                </div>
                <div class="capability-status">
                  <i class="fas fa-check-circle text-success"></i>
                </div>
              </div>

              <div class="capability-item">
                <div class="capability-icon">
                  <i class="fas fa-cut"></i>
                </div>
                <div class="capability-info">
                  <h5><%= t('manufacturer.profile.capabilities.garment.title') %></h5>
                  <p><%= t('manufacturer.profile.capabilities.garment.description') %></p>
                </div>
                <div class="capability-status">
                  <i class="fas fa-check-circle text-success"></i>
                </div>
              </div>

              <div class="capability-item">
                <div class="capability-icon">
                  <i class="fas fa-palette"></i>
                </div>
                <div class="capability-info">
                  <h5><%= t('manufacturer.profile.capabilities.dyeing.title') %></h5>
                  <p><%= t('manufacturer.profile.capabilities.dyeing.description') %></p>
                </div>
                <div class="capability-status">
                  <i class="fas fa-check-circle text-success"></i>
                </div>
              </div>

              <div class="capability-item">
                <div class="capability-icon">
                  <i class="fas fa-medal"></i>
                </div>
                <div class="capability-info">
                  <h5><%= t('manufacturer.profile.capabilities.quality.title') %></h5>
                  <p><%= t('manufacturer.profile.capabilities.quality.description') %></p>
                </div>
                <div class="capability-status">
                  <i class="fas fa-check-circle text-success"></i>
                </div>
              </div>

              <div class="capability-item">
                <div class="capability-icon">
                  <i class="fas fa-shipping-fast"></i>
                </div>
                <div class="capability-info">
                  <h5><%= t('manufacturer.profile.capabilities.delivery.title') %></h5>
                  <p><%= t('manufacturer.profile.capabilities.delivery.description') %></p>
                </div>
                <div class="capability-status">
                  <i class="fas fa-check-circle text-success"></i>
                </div>
              </div>

              <div class="capability-item">
                <div class="capability-icon">
                  <i class="fas fa-tools"></i>
                </div>
                <div class="capability-info">
                  <h5><%= t('manufacturer.profile.capabilities.custom.title') %></h5>
                  <p><%= t('manufacturer.profile.capabilities.custom.description') %></p>
                </div>
                <div class="capability-status">
                  <i class="fas fa-check-circle text-success"></i>
                </div>
              </div>
            </div>
          </div>
        </div>

        <!-- Contact Information Card -->
        <div class="profile-card">
          <div class="profile-card-header">
            <h3 class="profile-card-title">
              <i class="fas fa-address-book"></i>
              <%= t('manufacturer.profile.contact.title') %>
            </h3>
          </div>
          <div class="profile-card-content">
            <div class="contact-info-grid">
              <div class="contact-item">
                <div class="contact-icon">
                  <i class="fas fa-envelope"></i>
                </div>
                <div class="contact-details">
                  <label><%= t('manufacturer.profile.contact.email') %>:</label>
                  <a href="#" id="contactEmail">-</a>
                </div>
              </div>

              <div class="contact-item">
                <div class="contact-icon">
                  <i class="fas fa-phone"></i>
                </div>
                <div class="contact-details">
                  <label><%= t('manufacturer.profile.contact.phone') %>:</label>
                  <a href="#" id="contactPhone">-</a>
                </div>
              </div>

              <div class="contact-item">
                <div class="contact-icon">
                  <i class="fas fa-globe"></i>
                </div>
                <div class="contact-details">
                  <label><%= t('manufacturer.profile.contact.website') %>:</label>
                  <a href="#" target="_blank" id="contactWebsite">-</a>
                </div>
              </div>

              <div class="contact-item">
                <div class="contact-icon">
                  <i class="fas fa-map-marker-alt"></i>
                </div>
                <div class="contact-details">
                  <label><%= t('manufacturer.profile.contact.address') %>:</label>
                  <address id="contactAddress">-</address>
                </div>
              </div>
            </div>

            <!-- Social Media Links -->
            <% if (user.socialMedia && (user.socialMedia.linkedin || user.socialMedia.facebook || user.socialMedia.instagram || user.socialMedia.telegram)) { %>
              <div class="social-media-section">
                <h5><%= t('manufacturer.profile.contact.socialMedia') %></h5>
                <div class="social-links">
                  <% if (user.socialMedia.linkedin) { %>
                    <a href="<%= user.socialMedia.linkedin %>" target="_blank" class="social-link linkedin">
                      <i class="fab fa-linkedin"></i>
                    </a>
                  <% } %>
                  <% if (user.socialMedia.facebook) { %>
                    <a href="<%= user.socialMedia.facebook %>" target="_blank" class="social-link facebook">
                      <i class="fab fa-facebook"></i>
                    </a>
                  <% } %>
                  <% if (user.socialMedia.instagram) { %>
                    <a href="<%= user.socialMedia.instagram %>" target="_blank" class="social-link instagram">
                      <i class="fab fa-instagram"></i>
                    </a>
                  <% } %>
                  <% if (user.socialMedia.telegram) { %>
                    <a href="https://t.me/<%= user.socialMedia.telegram.replace('@', '') %>" target="_blank" class="social-link telegram">
                      <i class="fab fa-telegram"></i>
                    </a>
                  <% } %>
                </div>
              </div>
            <% } %>
          </div>
        </div>
      </div>

      <!-- Right Column -->
      <div class="profile-right-column">
        
        <!-- Performance Metrics Card -->
        <div class="profile-card">
          <div class="profile-card-header">
            <h3 class="profile-card-title">
              <i class="fas fa-chart-line"></i>
              <%= t('manufacturer.profile.performance.title') %>
            </h3>
          </div>
          <div class="profile-card-content">
            <div class="metrics-grid">
              <div class="metric-item">
                <div class="metric-icon sales">
                  <i class="fas fa-dollar-sign"></i>
                </div>
                <div class="metric-details">
                  <div class="metric-value" id="totalSales">$0</div>
                  <div class="metric-label"><%= t('manufacturer.profile.performance.metrics.totalSales') %></div>
                  <div class="metric-change positive">
                    <i class="fas fa-arrow-up"></i>
                    <span id="salesChange">+0%</span>
                  </div>
                </div>
              </div>

              <div class="metric-item">
                <div class="metric-icon orders">
                  <i class="fas fa-shopping-cart"></i>
                </div>
                <div class="metric-details">
                  <div class="metric-value" id="completedOrders">0</div>
                  <div class="metric-label"><%= t('manufacturer.profile.performance.metrics.completedOrders') %></div>
                  <div class="metric-change positive">
                    <i class="fas fa-arrow-up"></i>
                    <span id="ordersChange">+0%</span>
                  </div>
                </div>
              </div>

              <div class="metric-item">
                <div class="metric-icon rating">
                  <i class="fas fa-star"></i>
                </div>
                <div class="metric-details">
                  <div class="metric-value" id="averageRating">0.0</div>
                  <div class="metric-label"><%= t('manufacturer.profile.performance.metrics.averageRating') %></div>
                  <div class="metric-reviews">
                    <span id="totalReviews">0</span> <%= t('manufacturer.profile.performance.metrics.reviews') %>
                  </div>
                </div>
              </div>

              <div class="metric-item">
                <div class="metric-icon response">
                  <i class="fas fa-clock"></i>
                </div>
                <div class="metric-details">
                  <div class="metric-value" id="responseTime">0h</div>
                  <div class="metric-label"><%= t('manufacturer.profile.performance.metrics.responseTime') %></div>
                  <div class="metric-change positive">
                    <i class="fas fa-arrow-down"></i>
                    <span id="responseChange">-0%</span>
                  </div>
                </div>
              </div>
            </div>

            <!-- Performance Chart -->
            <div class="performance-chart-container">
              <canvas id="performanceChart" width="400" height="200"></canvas>
            </div>
          </div>
        </div>

        <!-- Recent Products Card -->
        <div class="profile-card">
          <div class="profile-card-header">
            <h3 class="profile-card-title">
              <i class="fas fa-box"></i>
              <%= t('manufacturer.profile.recentProducts.title') %>
            </h3>
            <a href="/manufacturer/products" class="profile-card-action">
              <%= t('manufacturer.profile.recentProducts.viewAll') %>
              <i class="fas fa-arrow-right"></i>
            </a>
          </div>
          <div class="profile-card-content">
            <div id="recent-products" class="recent-items-list">
              <!-- Products will be loaded here -->
            </div>
          </div>
        </div>

        <!-- Recent Orders Card -->
        <div class="profile-card">
          <div class="profile-card-header">
            <h3 class="profile-card-title">
              <i class="fas fa-shopping-cart"></i>
              <%= t('manufacturer.profile.recentOrders.title') %>
            </h3>
            <a href="/manufacturer/orders" class="profile-card-action">
              <%= t('manufacturer.profile.recentOrders.viewAll') %>
              <i class="fas fa-arrow-right"></i>
            </a>
          </div>
          <div class="profile-card-content">
            <div id="recent-orders" class="recent-items-list">
              <!-- Orders will be loaded here -->
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</main>

<!-- Profile Share Modal -->
<div class="modal-overlay" id="shareProfileModal">
  <div class="modal-content">
    <div class="modal-header">
      <h3 class="modal-title">
        <i class="fas fa-share"></i>
        <%= t('manufacturer.profile.share.title') %>
      </h3>
      <button class="modal-close" onclick="closeModal('shareProfileModal')">
        <i class="fas fa-times"></i>
      </button>
    </div>
    <div class="modal-body">
      <div class="share-options">
        <div class="share-url-section">
          <label><%= t('manufacturer.profile.share.url') %>:</label>
          <div class="url-input-group">
            <input type="text" id="profileUrl" value="https://slex.uz/manufacturer/<%= user._id %>" readonly>
            <button class="orders-btn orders-btn-outline" id="copyUrlBtn">
              <i class="fas fa-copy"></i>
              <%= t('manufacturer.profile.actions.copy') %>
            </button>
          </div>
        </div>

        <div class="qr-code-section">
          <h5><%= t('manufacturer.profile.share.qrCode') %>:</h5>
          <div class="qr-code-container">
            <canvas id="qrCodeCanvas" width="200" height="200"></canvas>
          </div>
          <button class="orders-btn orders-btn-outline" id="downloadQrBtn">
            <i class="fas fa-download"></i>
            <%= t('manufacturer.profile.share.downloadQr') %>
          </button>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- Fix double scroll by adding profile-page class to body -->
<script>
document.addEventListener('DOMContentLoaded', function() {
  // Add profile-page class to body to enable specific CSS fixes
  document.body.classList.add('profile-page-body');
  document.documentElement.classList.add('profile-page-html');
});
</script>

<!-- Professional Unified Header Management -->
<script src="/js/manufacturer/manufacturer-header.js"></script>

<!-- JavaScript -->
<script src="/js/chart.min.js"></script>
<script src="/js/qrcode.min.js"></script>

<!-- Profile Translations -->
